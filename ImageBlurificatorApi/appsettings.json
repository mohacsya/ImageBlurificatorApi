{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ImageProcessingValidation": {
        "MaxPixels": 20000000,
        "MaxFileBytes": 40000000,
        "MaxDecodedBytes": 40000000,
        "AllowedEncodings": [ "PNG", "JPG" ],
        "AllowedContentTypes": [ "image/png", "image/jpeg" ]
    }
}
